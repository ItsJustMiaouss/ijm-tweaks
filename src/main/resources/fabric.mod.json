	"description": "A Fabric mod that add a few tweaks for the client side of Minecraft.",
	"authors": [
		"ItsJustMiaouss"
	],
	"contact": {
		"sources": "https://github.com/ItsJustMiaouss/ijm-tweaks"
	},
	"license": "MIT",
	"icon": "assets/ijmtweaks/icon.png",
	"environment": "client",
	"entrypoints": {
		"client": [
			"com.itsjustmiaouss.ijmtweaks.IJMTweaks"
		],
		"modmenu": [
